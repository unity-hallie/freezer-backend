# Environment variables for Droplet deployment
# Copy to .env on your server

# Database
DB_PASSWORD=your-secure-database-password-here

# JWT Secret - Generate with: python -c "import secrets; print(secrets.token_urlsafe(64))"
JWT_SECRET_KEY=your-secure-jwt-secret-key-64-chars-minimum

# CORS Origins - Your domain
CORS_ORIGINS=https://your-domain.com,https://www.your-domain.com

# Optional: Email for SSL certificates
SSL_EMAIL=your-email@example.com
DOMAIN=your-domain.com